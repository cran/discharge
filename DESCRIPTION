Package: discharge
Type: Package
Title: Fourier Analysis of Discharge Data
Version: 1.0.0
Encoding: UTF-8
Date: 2019-03-06
Authors@R: c(
    person("Samarth", "Shah", email = "sbshah10@asu.edu", role = c("aut","cre")),
    person("Albert", "Ruhi", role = "aut"),
    person("Future H2O", role = "cph")
